{
    "name": "somecode/snack",
    "description": "Incredibly simple tool for automatically generating API documentation for Laravel",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Somecode\\Restify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somecode\\Restify\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mikhail Protasevich",
            "email": "m1nyasha@yandex.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "somecode/openapi-builder": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.16",
        "orchestra/testbench": "^9.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Somecode\\Restify\\Providers\\RestifyServiceProvider"
            ]
        }
    }
}
